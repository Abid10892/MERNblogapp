{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      
      {
        "src": "/",
        "dest": "frontend/build/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "frontend/build/$1"
      }
    ]
  }