{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      
      {
        "src": "/",
        "dest": "index.html"
      },
      {
        "src": "/(.+)",
        "dest": "build/$1"
      }
    ],

    "rewrites": [{ "source": "/(.*)", "destination": "index.js" }]
  }